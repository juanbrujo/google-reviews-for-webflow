{
  "name": "google-reviews-for-webflow",
  "version": "2.0.0",
  "description": "Free, open-source widget to embed Google Reviews in Webflow CMS sites",
  "private": true,
  "main": "assets/js/main.js",
  "type": "module",
  "scripts": {
    "dev": "netlify dev",
    "build": "echo 'No build step required - pure static HTML/CSS/JS'",
    "start": "netlify dev",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "echo 'Add eslint for linting'",
    "format": "prettier --write '**/*.{html,css,js,json,md}'",
    "serve": "python3 -m http.server 8000"
  },
  "keywords": [
    "webflow",
    "google-reviews",
    "widget",
    "integration",
    "cms",
    "netlify",
    "places-api",
    "carousel",
    "responsive"
  ],
  "author": "Jorge Epu√±an H. (https://github.com/juanbrujo)",
  "contributors": [
    "Google Reviews for Webflow Contributors"
  ],
  "license": "MIT",
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "happy-dom": "^20.0.11",
    "netlify-cli": "^17.0.0",
    "vitest": "^4.0.16"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/google-reviews-for-webflow.git"
  },
  "homepage": "https://google-reviews-for-webflow.netlify.app/",
  "bugs": {
    "url": "https://github.com/your-org/google-reviews-for-webflow/issues"
  }
}
